<?xml version="1.0" encoding="ISO-8859-1" ?>
<package>
  <name>HTTP_Request</name>
  <summary>Provides an easy way to perform HTTP requests</summary>
  <description>
	Supports GET/POST/HEAD/TRACE/PUT/DELETE, Basic authentication, Proxy,
	Proxy Authentication etc.
  </description>
  <maintainers>
      <maintainer>
        <user>heyesr</user>
        <name>Richard Heyes</name>
        <email>heyesr@php.net</email>
        <role>lead</role>
      </maintainer>
  </maintainers>
  <release>
    <version>1.0</version>
    <date>2002-02-10</date>
    <state>stable</state>
    <notes>
      This is the initial release of the HTTP_Request package.
    </notes>
    <filelist>
      <dir name="/" baseinstalldir="HTTP">
        <file role="php">Request.php</file>
        <dir name="docs" role="doc">
            <file>example.php</file>
        </dir>
      </dir>
    </filelist>
    <deps>
      <dep type="pkg" rel="ge" version="1.0">Net_URL</dep>
      <dep type="pkg" rel="ge" version="1.0">Net_Socket</dep>
    </deps>
  </release>
</package>
